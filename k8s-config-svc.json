{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "shoe-store-svc",
    "namespace": "default"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "shoe-store",
        "protocol": "TCP",
        "port": 8080,
        "targetPort": 8080
      }
    ],
    "selector": {
      "app": "shoe-store"
    }
  }
}